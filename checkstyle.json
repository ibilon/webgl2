{
	"defineCombinations": [],
	"defaultSeverity": "INFO",
	"baseDefines": [],
	"exclude": {},
	"checks": [
		{
			"props": {},
			"type": "Anonymous"
		},
		{
			"props": {
				"spaceBefore": false,
				"spaceInside": false
			},
			"type": "ArrayAccess"
		},
		{
			"props": {},
			"type": "ArrayLiteral"
		},
		{
			"props": {
				"format": "^(e|t|ex|[a-z][a-z][a-zA-Z]+)$"
			},
			"type": "CatchParameterName"
		},
		{
			"props": {
				"policy": "aligned",
				"allowSingleline": true
			},
			"type": "ConditionalCompilation"
		},
		{
			"props": {
				"ignoreExtern": true,
				"format": "^[a-z][a-zA-Z0-9]*$",
				"tokens": [
					"NOTINLINE"
				]
			},
			"type": "ConstantName"
		},
		{
			"props": {
				"ignoreExtern": true,
				"format": "^[A-Z0-9]*$",
				"tokens": [
					"INLINE"
				]
			},
			"type": "ConstantName"
		},
		{
			"props": {
				"thresholds": [
					{
						"complexity": 11,
						"severity": "WARNING"
					},
					{
						"complexity": 21,
						"severity": "ERROR"
					}
				]
			},
			"type": "CyclomaticComplexity"
		},
		{
			"props": {},
			"type": "DefaultComesLast"
		},
		{
			"props": {},
			"type": "ERegLiteral"
		},
		{
			"props": {
				"requireEmptyLineAfterPackage": true,
				"requireEmptyLineAfterInterface": true,
				"requireEmptyLineAfterAbstract": true,
				"allowEmptyLineAfterSingleLineComment": true,
				"max": 1,
				"requireEmptyLineAfterClass": true,
				"allowEmptyLineAfterMultiLineComment": true
			},
			"type": "EmptyLines"
		},
		{
			"props": {
				"enforceEmptyPackage": false
			},
			"type": "EmptyPackage"
		},
		{
			"props": {
				"option": "upperCase"
			},
			"type": "HexadecimalLiteral"
		},
		{
			"props": {
				"character": "tab",
				"ignorePattern": "^$"
			},
			"type": "IndentationCharacter"
		},
		{
			"props": {
				"allowProperties": false,
				"allowMarkerInterfaces": true
			},
			"type": "Interface"
		},
		{
			"props": {
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"INTERFACE_DEF",
					"TYPEDEF_DEF",
					"FUNCTION",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				],
				"ignoreEmptySingleline": true,
				"option": "nl"
			},
			"type": "LeftCurly"
		},
		{
			"props": {
				"ignoreExtern": true,
				"format": "^[a-z][a-zA-Z0-9]*$",
				"tokens": []
			},
			"type": "LocalVariableName"
		},
		{
			"props": {
				"ignoreExtern": false,
				"format": "^[a-zA-Z0-9_]*$",
				"tokens": [
					"PUBLIC",
					"CLASS",
					"TYPEDEF",
					"PRIVATE",
					"ABSTRACT",
					"ENUM"
				]
			},
			"type": "MemberName"
		},
		{
			"props": {
				"max": 500
			},
			"type": "MethodLength"
		},
		{
			"props": {
				"ignoreExtern": false,
				"format": "^[a-z][a-zA-Z0-9_]*$",
				"tokens": [
					"PUBLIC",
					"PRIVATE"
				]
			},
			"type": "MethodName"
		},
		{
			"props": {
				"modifiers": [
					"MACRO",
					"OVERRIDE",
					"PUBLIC_PRIVATE",
					"STATIC",
					"INLINE",
					"DYNAMIC"
				]
			},
			"type": "ModifierOrder"
		},
		{
			"props": {
				"minLength": 2,
				"ignore": "^\\s+$",
				"allowDuplicates": 2
			},
			"type": "MultipleStringLiterals"
		},
		{
			"props": {},
			"type": "MultipleVariableDeclarations"
		},
		{
			"props": {
				"allowSingleLineStatement": true,
				"tokens": [
					"FOR",
					"IF",
					"ELSE_IF",
					"WHILE",
					"DO_WHILE"
				]
			},
			"type": "NeedBraces"
		},
		{
			"props": {
				"max": 1
			},
			"type": "NestedForDepth"
		},
		{
			"props": {
				"max": 1
			},
			"type": "NestedIfDepth"
		},
		{
			"props": {
				"max": 1
			},
			"type": "NestedTryDepth"
		},
		{
			"props": {
				"option": "questionmark"
			},
			"type": "NullableParameter"
		},
		{
			"props": {
				"ternaryOpPolicy": "around",
				"unaryOpPolicy": "none",
				"boolOpPolicy": "around",
				"intervalOpPolicy": "none",
				"assignOpPolicy": "around",
				"functionArgPolicy": "none",
				"bitwiseOpPolicy": "around",
				"arithmeticOpPolicy": "around",
				"compareOpPolicy": "around",
				"arrowPolicy": "around"
			},
			"type": "OperatorWhitespace"
		},
		{
			"props": {
				"tokens": [
					"=",
					"+",
					"-",
					"*",
					"/",
					"%",
					">",
					"<",
					">=",
					"<=",
					"==",
					"!=",
					"&",
					"|",
					"^",
					"&&",
					"||",
					"<<",
					">>",
					">>>",
					"+=",
					"-=",
					"*=",
					"/=",
					"%=",
					"<<=",
					">>=",
					">>>=",
					"|=",
					"&=",
					"^=",
					"...",
					"=>",
					"++",
					"--"
				],
				"option": "eol"
			},
			"type": "OperatorWrap"
		},
		{
			"props": {
				"ignoreExtern": true,
				"format": "^(_|[a-z][a-zA-Z0-9]*$)",
				"tokens": []
			},
			"type": "ParameterName"
		},
		{
			"props": {
				"enforcePrivate": false,
				"enforcePublic": false,
				"enforcePublicPrivate": false
			},
			"type": "RedundantModifier"
		},
		{
			"props": {
				"allowEmptyReturn": true,
				"enforceReturnType": true
			},
			"type": "Return"
		},
		{
			"props": {
				"ignoreFormat": "^$",
				"max": 4
			},
			"type": "ReturnCount"
		},
		{
			"props": {
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"TYPEDEF_DEF",
					"INTERFACE_DEF",
					"FUNCTION",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				],
				"option": "aloneorsingle"
			},
			"type": "RightCurly"
		},
		{
			"props": {
				"commaPolicy": "after",
				"semicolonPolicy": "after",
				"dotPolicy": "none"
			},
			"type": "SeparatorWhitespace"
		},
		{
			"props": {
				"tokens": [
					",",
					"."
				],
				"option": "eol"
			},
			"type": "SeparatorWrap"
		},
		{
			"props": {},
			"type": "SimplifyBooleanExpression"
		},
		{
			"props": {},
			"type": "SimplifyBooleanReturn"
		},
		{
			"props": {
				"spaceIfCondition": 0,
				"spaceAroundBinop": true,
				"spaceForLoop": 0,
				"ignoreRangeOperator": true,
				"spaceWhileLoop": 0,
				"spaceCatch": 0,
				"spaceSwitchCase": 0,
				"noSpaceAroundUnop": true
			},
			"type": "Spacing"
		},
		{
			"props": {
				"policy": "doubleAndInterpolation",
				"allowException": true
			},
			"type": "StringLiteral"
		},
		{
			"props": {
				"format": "^\\s*(TODO|FIXME|HACK|XXX|BUG)",
				"severity": "WARNING"
			},
			"type": "TODOComment"
		},
		{
			"props": {
				"ignorePattern": "^$"
			},
			"type": "TabForAligning"
		},
		{
			"props": {},
			"type": "Trace"
		},
		{
			"props": {},
			"type": "TrailingWhitespace"
		},
		{
			"props": {
				"ignoreExtern": true,
				"format": "^[A-Z]+[a-zA-Z0-9]*$",
				"tokens": []
			},
			"type": "TypeName"
		},
		{
			"props": {},
			"type": "UnusedLocalVar"
		},
		{
			"props": {
				"tokens": [
					",",
					";"
				]
			},
			"type": "WhitespaceAfter"
		},
		{
			"props": {
				"tokens": [
					"=",
					"+",
					"-",
					"*",
					"/",
					"%",
					">",
					"<",
					">=",
					"<=",
					"==",
					"!=",
					"&",
					"|",
					"^",
					"&&",
					"||",
					"<<",
					">>",
					">>>",
					"+=",
					"-=",
					"*=",
					"/=",
					"%=",
					"<<=",
					">>=",
					">>>=",
					"|=",
					"&=",
					"^=",
					"=>"
				]
			},
			"type": "WhitespaceAround"
		}
	]
}
